<% if music.favorited_by?(current_user) %>
  <%= link_to music_favorites_path(music), method: :delete, class: "text-warning",remote: true do %><i class="fas fa-star fa-lg"><%= music.favorites.count %></i><% end %>
<% else %>
  <%= link_to music_favorites_path(music), method: :post, remote: true do %><i class="far fa-star fa-lg"><%= music.favorites.count %></i><% end %>
<% end %>