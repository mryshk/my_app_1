
<div class="card bg-dark text-white" style="height: 750px">
  <div id="theTarget">
    <img class=" image1 bd-placeholder-img bd-placeholder-img-lg card-img" src="assets/image1.jpg" width="100%" height="800"></img>
    <img class=" image2 bd-placeholder-img bd-placeholder-img-lg card-img" src="assets/image6.jpg" width="100%" height="800"></img>
    <img class=" image3 bd-placeholder-img bd-placeholder-img-lg card-img" src="assets/image3.jpg" width="100%" height="800"></img>
    <img class=" image4 bd-placeholder-img bd-placeholder-img-lg card-img" src="assets/image5.jpg" width="100%" height="800"></img>
  </div>
  <div class="card-img-overlay text-center mt-4 ">
    <h1 class="card-title"><i class="fas fa-headphones-alt">&nbsp;Musicers</i></h1>
    <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
    <p class="card-text">Last updated 3 mins ago</p>
  </div>
</div>
<div class="container">
 <h1 class="my-5 text-center"><i class="fas fa-headphones-alt">Musics</i></h1>

 <%=  render "index", musics: @musics %>

  <div class="row mt-5">
    <div class="col-2">
      <button class="btn btn-primary ">
        <%= link_to :back ,class: "text-light" do%><i class="fas fa-backward">&nbsp;Back</i><% end %>
      </button>
    </div>
    <div class="mx-auto">
      <%= paginate @musics %>
    </div>
  </div>
</div>

