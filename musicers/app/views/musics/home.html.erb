<div class=" card bg-dark text-white" id="area-1">
  <img class="bd-placeholder-img bd-placeholder-img-lg card-img"src="assets/image1.jpg" width="100%" height="800"></img>
  <div class="card-img-overlay text-center">
    <h1 class="card-title  mt-5"><i class="fas fa-house-user">&nbsp;Home</i></h1>
  </div>
</div>
<div class="container" style="margin-top:300px;">
  <h1 class="text-center my-5" ><i class="fas fa-user-clock">&nbsp;Timeline</i></h1>

  <%= render "musics/index",musics: @musics %>

  <div class="row mt-5">
    <div class="col-2">
      <button class="btn btn-primary ">
        <%= link_to :back ,class: "text-light" do%><i class="fas fa-backward">&nbsp;Back</i><% end %>
      </button>
    </div>
    <div class="mx-auto">
      <%= paginate @musics %>
    </div>
  </div>
</div>